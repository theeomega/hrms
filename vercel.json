{
  "crons": [
    {
      "path": "/api/cron/mark-leaves",
      "schedule": "5 0 * * *"
    },
    {
      "path": "/api/cron/mark-absent",
      "schedule": "55 23 * * *"
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
